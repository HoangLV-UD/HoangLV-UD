	<queryString>
		<![CDATA[SELECT
     HOADON."MAHD" AS HOADON_MAHD,
     HOADON."MAKH" AS HOADON_MAKH,
     HOADON."TENKH" AS HOADON_TENKH,
     HOADON."SDT" AS HOADON_SDT,
     HOADON."MANV" AS HOADON_MANV,
     HOADON."NGAYTAO" AS HOADON_NGAYTAO,
     HOADON."TRANGTHAI" AS HOADON_TRANGTHAI,
     HOADON."HTTHANHTOAN" AS HOADON_HTTHANHTOAN,
     HOADON."TIENSHIP" AS HOADON_TIENSHIP,
     HOADON."TONGTIEN" AS HOADON_TONGTIEN,
     HOADON."DIACHI" AS HOADON_DIACHI,
     HOADON."GHICHU" AS HOADON_GHICHU,
     KHACHHANG."MAKH" AS KHACHHANG_MAKH,
     KHACHHANG."HOTEN" AS KHACHHANG_HOTEN,
     NHANVIEN."MANV" AS NHANVIEN_MANV,
     NHANVIEN."HOTEN" AS NHANVIEN_HOTEN,
     CTHOADON."MAHD" AS CTHOADON_MAHD,
     CTHOADON."MASP" AS CTHOADON_MASP,
     CTHOADON."SOLUONG" AS CTHOADON_SOLUONG,
     CTHOADON."DONVI" AS CTHOADON_DONVI,
     CTHOADON."DONGIA" AS CTHOADON_DONGIA,
     CTHOADON."THANHTIEN" AS CTHOADON_THANHTIEN,
     CTHOADON."TRANGTHAI" AS CTHOADON_TRANGTHAI,
     CTHOADON."GHICHU" AS CTHOADON_GHICHU,
     KHACHHANG."SDT" AS KHACHHANG_SDT
FROM
     "dbo"."KHACHHANG" KHACHHANG INNER JOIN "dbo"."HOADON" HOADON ON KHACHHANG."MAKH" = HOADON."MAKH"
     INNER JOIN "dbo"."NHANVIEN" NHANVIEN ON HOADON."MANV" = NHANVIEN."MANV"
     INNER JOIN "dbo"."CTHOADON" CTHOADON ON HOADON."MAHD" = CTHOADON."MAHD"
WHERE
     HOADON."MAHD" = $P{MAHD2}]]>
	</queryString>
